mixin videoHome(nameClass, video, src, alt, widthImg, heightImg, text, ifElse)
  div(class=`${nameClass}`)
    -if (ifElse !== '')
        iframe(id="iframeVideo" src=`${video}` title="Iframe deportes betfran" width='100%' height='348px' frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media gyroscope; picture-in-picture" allowfullscreen)
    -else
        img(class='img-fluid img-video' value=`${video}` src=`${src}`, alt=`${alt}`, width=`${widthImg}`, height=`${heightImg}`)
        p=`${text}`

-for (var i = 0; i < videoHome.length; i++)
  -var d = videoHome[i]
  +videoHome(d.nameClass, d.video, d.src, d.alt, d.widthImg, d.heightImg, d.text, d.ifElse)